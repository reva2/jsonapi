{
    "name": "reva2/jsonapi",
    "description": "Library for JSON API implementation",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey Revenko",
            "email": "dedsemen@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.5.9 || ^7.0",
        "neomerx/json-api": "^0.8",
        "symfony/http-foundation": "^3.0",
        "doctrine/common": "^2.6",
        "symfony/property-access": "^3.1",
        "symfony/validator": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.4",
        "scrutinizer/ocular": "^1.3",
        "squizlabs/php_codesniffer": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "Reva2\\JsonApi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Reva2\\JsonApi\\Tests\\": "tests"
        }
    }
}
